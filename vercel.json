{
    "version": 2,
    "builds": [
      {
        "src": "script.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/script.js" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
  }
  